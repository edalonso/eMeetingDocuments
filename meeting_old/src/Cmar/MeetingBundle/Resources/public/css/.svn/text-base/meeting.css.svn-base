/*MENU*/
#container{  
    margin:auto;  
    width:200%;
    height: 125%;  
    margin-top: 15px;
}  
#container ul{  
    list-style:none;  
    list-style-position:outside;  
}  
#container ul.menu li{  
    float:left;  
    margin-right:8px;  
    margin-bottom:-1px;  
}  
#container ul.menu li{  
    font-weight:700;  
    display:block;  
    padding:10px 15px;  
    background:#6DAABC;  
    margin-bottom:-1px;  
    border:1px solid #D7E6EB;  
    border-width:1px 1px 1px 1px;  
    position:relative;  
    color:#e4e1cd;  
    cursor:pointer;  
    border-radius:3px 3px 0 0;  
    -moz-border-radius:3px 3px 0 0;  
    -webkit-border-radius:3px 3px 0 0;  
}  
#container ul.menu li:hover{  
    color:#fff;  
}  
#container ul.menu li.active{  
    background:#6DAABC;  
    top:1px;  
    border-bottom:0;  
    color:#fff;  
}  
/* END MENU*/  

.wrapper{
  
    height: 200px;
    width: 50%;
    padding: 0px;
    margin: 0px;
}
  
/*CONTENT*/  
.content{  
    margin:0px;  
    background:#efefef;  
    background:#6DAABC;  
    border:1px solid #D7E6EB;  
    text-align:left;
    padding-bottom:0px;
    font-size:14px;  
    width: 321px;
    height:70%;
    color:#8C8C8C;  
    border-radius:5px 5px 5px 5px;  
    -moz-border-radius:5px 5px 5px 5px;  
    -webkit-border-radius:5px 5px 5px 5px;  
}  
.content h3{  
    line-height:0em;
    vertical-align:middle;  
    height:20px;  
    padding:0px 0px 0px 122px;  
    font-size:25px;  
    color:#A07D17;  
} 
/* END CONTENT*/  
  
/* COMMON USERS */
.content.common_users h2{  
    font-size: 20px;
    text-shadow: 0 1px 0 black;
    color: #fff;
}
.content.common_users h3{  
    background:transparent no-repeat scroll left top;  
}  
.content.common_users{  
    display:block;  
}  
/* END COMMON USERS */  
  
/* GROUP 1 */  
.content.group h3{  
    background:transparent no-repeat scroll left top;  
}  
.content.group{  
    display:none;  
}  
/* END GROUP 1 */  
  
/* All USERS */  
.content.all_users h3{  
    background:transparent no-repeat scroll left top;  
}  
  
.content.all_users a{  
    color:#498BBF;  
}  
