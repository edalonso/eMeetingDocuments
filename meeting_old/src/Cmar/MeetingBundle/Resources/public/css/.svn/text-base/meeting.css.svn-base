/* CSS PARA SORTABLE DE JQUERY */
#sortable {
    list-style-type: none;
    margin: 0;
    padding: 0;
}

.np {
    background: url("../images/bg_form.jpg") no-repeat scroll 100% 100% #6DAABC;
    color: #FFFFFF;
    font-weight: bolder;
    margin: 0px 0 5px;
    padding: 40px 120px;
