/* VENTANA EMERGENTE */
.ui-corner-all {
    border-radius: 4px;
}

.ui-widget {
    font-family: Trebuchet MS,Tahoma,Verdana,Arial,sans-serif;
    font-size: 1.1em;
}

.ui-corner-all, .ui-corner-bottom, .ui-corner-right, .ui-corner-br {
    border-bottom-right-radius: 4px;
}

.ui-corner-all, .ui-corner-bottom, .ui-corner-left, .ui-corner-bl {
    border-bottom-left-radius: 4px;
}

.ui-corner-all, .ui-corner-top, .ui-corner-right, .ui-corner-tr {
    border-top-right-radius: 4px;
}

.ui-corner-all, .ui-corner-top, .ui-corner-left, .ui-corner-tl {
    border-top-left-radius: 4px;
}

.ui-widget-content {
    background: repeat-x scroll 50% 50% #FFFFFF;
    border: 1px solid #AAAAAA;
    color: #222222;
}

.ui-widget {
    font-family: Verdana,Arial,sans-serif;
    font-size: 1.1em;
}

.ui-widget-header {
    background: repeat-x scroll 50% 50% #A07D17;
    border: 1px solid #A07D17;
    color: #FFFFFF;
    font-weight: bold;
}

.ui-resizable {
    position: relative;
}

.ui-dialog {
    overflow: hidden;
    padding: 0.2em;
    position: absolute;
}

.ui-dialog-content {
    background: #6DAABC;
    border: 0 none;
    overflow: hidden;
    padding: 0px;
    margin: 0px;
    position: relative;
}

.ui-widget-header {
    background: repeat-x scroll 50% 50% #A07D17;
    border: 1px solid #A07D17;
    color: #FFFFFF;
    font-weight: bold;
}

.ui-dialog-titlebar-close {
    display: inline-table;
    margin: 1px 6px;
    position: absolute;
    top: 4px;
    right:0;
}

.ui-widget-shadow{
    box-shadow: -10px -10px 0px #000;
    -moz-box-shadow:-10px -10px 0px #000;
    -webkit-box-shadow:-10px -10px 0px #000;
}

input[placeholder], [placeholder], *[placeholder]
{
    color:grey;
    font-family: Verdana,Arial,sans-serif;
}

.dialogWithShadow
{
    box-shadow: 0px 0px 64px rgba(0, 0, 0, 0.9);  
    -webkit-box-shadow: 0px 0px 64px rgba(0, 0, 0, 0.9);  
    -moz-box-shadow: 0px 0px 64px rgba(0, 0, 0, 0.9);
    left: 40% !important;
}

.dialogWithShadowAdoTest
{
    height: 218px !important;
    box-shadow: 0px 0px 64px rgba(0, 0, 0, 0.9);
    -webkit-box-shadow: 0px 0px 64px rgba(0, 0, 0, 0.9);
    -moz-box-shadow: 0px 0px 64px rgba(0, 0, 0, 0.9);
    left: 30% !important;
}


/* Mensajes de Aviso u Error */

.mbox-cont {
    padding-bottom: 3px;
    padding-top: 3px;
}

.mbox-cont {
    border-radius: 4px 4px 4px 4px;
    box-shadow: 0 2px 36px rgba(0, 0, 0, 0.7);
    -moz-transition: all 0.218s ease 0s;
    -webkit-box-shadow: 0px 0px 36px rgba(0, 0, 0, 0.7);  
    -moz-box-shadow: 0px 0px 36px rgba(0, 0, 0, 0.7); 
}

.mbox-cont {
    background: none repeat scroll 0 0 #124557;
    border: 1px solid #124557;
    color: #FFF;
    font-size: 14px;
    font-weight: bold;
    text-align: center;
    padding: 9px 15px;
    position: absolute;
    margin-left: auto;
    margin-right: auto;
    left:0;
    right:0;
    width: 120px;
    top: 155px;
    z-index: 1000;
}


/* CAPA SUPERIOR DE OPACIDAD PARA LOS EMMETINGS */
.visible {display:block;}
.invisible {display:none;}

#capasuperior {
    position: absolute;
    top:0px;
    left: 0px;
    width: 100%;
    height: 100%;
    z-index: 99;
    opacity: 0.6;filter: alpha(opacity=60);
    background: #FFF;
    border-radius: 10px 0px 0px 10px;
}